println x ->
    print x
    print "\n"

println (1 + 2)       # 3
println (1 + 2.0)     # 3.0
println (1.0 + 2)     # 3.0
println (1.0 + 2.0)   # 3.0

x = 10
y = 20

dist = (x * x) + (y * y)
dist-wrong = x * x + y * y

println dist          # (10 * 10) + (20 * 20) => 500
println dist-wrong    # ((10 * 10) + 20) * 20 => 2400

println (5 / 2)       # 2
println (5.0 / 2)     # 2.5